{
  "name": "SIAP_4BIM_DATAS_AZUIS",
  "icon": "riGlobalLine",
  "version": "1.29.12",
  "drawflow": {
    "nodes": [
      { "id": "trig", "type": "BlockBasic", "label": "trigger", "position": { "x": 0, "y": 300 }, "data": { "type": "manual" } },
      { "id": "tab", "type": "BlockBasic", "label": "new-tab", "position": { "x": 200, "y": 300 }, "data": { "url": "https://siap.educacao.go.gov.br/DiarioEscolarListagem.aspx", "active": true } },
      { "id": "wait1", "type": "BlockBasic", "label": "delay", "position": { "x": 400, "y": 300 }, "data": { "delay": 8000 } },
      { "id": "bim", "type": "BlockBasic", "label": "forms", "position": { "x": 600, "y": 300 }, "data": { "type": "select", "selector": "#cphFuncionalidade_cphCampos_ddlBimestre", "value": "4" } },
      { "id": "list", "type": "BlockBasic", "label": "event-click", "position": { "x": 800, "y": 300 }, "data": { "selector": "#cphFuncionalidade_btnListar" } },
      { "id": "wait_load", "type": "BlockBasic", "label": "wait-condition", "position": { "x": 1000, "y": 300 }, "data": { "selector": ".modal-backdrop", "changeType": "element-removed", "timeout": 20000 } },
      { "id": "loop", "type": "BlockBasic", "label": "loop-elements", "position": { "x": 1250, "y": 300 }, "data": { "selector": ".dialog[style*='background-color: Blue'], .dialog.azul", "description": "Busca todas as datas azuis" } },
      { "id": "click_day", "type": "BlockBasic", "label": "event-click", "position": { "x": 1500, "y": 300 }, "data": { "selector": "$.loop.element" } },
      { "id": "save", "type": "BlockBasic", "label": "event-click", "position": { "x": 1750, "y": 300 }, "data": { "selector": "#cphFuncionalidade_btnAlterar" } }
    ],
    "edges": [
      { "id": "e1", "source": "trig", "target": "tab" },
      { "id": "e2", "source": "tab", "target": "wait1" },
      { "id": "e3", "source": "wait1", "target": "bim" },
      { "id": "e4", "source": "bim", "target": "list" },
      { "id": "e5", "source": "list", "target": "wait_load" },
      { "id": "e6", "source": "wait_load", "target": "loop" },
      { "id": "e7", "source": "loop", "target": "click_day" },
      { "id": "e8", "source": "click_day", "target": "save" },
      { "id": "e9", "source": "save", "target": "loop", "targetHandle": "loop-input-1" }
    ]
  }
}
